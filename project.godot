[application]

config/name="Mario Forever Cloud Engine"
run/main_scene="res://Stages/Menu.tscn"
boot_splash/image="res://GFX/Miscellaneous/Splash.png"
config/icon="res://icon.png"

[autoload]

Global="*res://Scripts/Global.gd"
MusicEngine="*res://Scripts/MusicEngine.gd"
GlobalEditor="*res://Scripts/GlobalEditor.gd"
MusicPlayer="*res://Objects/Core/MusicPlayer.tscn"

[debug]

settings/stdout/print_fps=true

